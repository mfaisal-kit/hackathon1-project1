{
  "version": 2,
  "builds": [
    {
      "src": "frontend_book/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend_book/build/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://muhammadfasial-deploy-ragchatbot.hf.space"
  }
}